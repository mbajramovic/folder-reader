{{folder-number 
    on-show-content='onGetFolderContent' class='folder-number-div'
}}
<div class="path-link">
    <a href="#" {{action 'onShowPathDialog' }} >Putanja do foldera (Administrator)</a>
</div>
{{alert-window 
    alert-show=model._alert.show
    alert-class=model._alert.class
    alert-message=model._alert.message
    alert-header=model._alert.header
    on-close='onCloseAlert'
    class='alert-win'
}}
